use io

start print_num | num is int
    out | "The number is: "
    out | num
    out | "\n"
end

start sum | a is int, b is int
    return a + b
end

start main
    index1 is int
    index2 is int

    index1 = -1
    index2 = (index1 * 3 - 1) % 5

    print_num | index1
    print_num | index2
end