use io

start print_num | num is int
    out | "The number is: "
    out | num
    out | "\n"
end

start sum | a is int, b is int
    return a + b
end

start main
    index1 is int
    index2 is int

    index1 = 2
    index2 = (2 - index1) * 2

	out | index1
	out | "\n"
	out | index2
	out | "\n"
end